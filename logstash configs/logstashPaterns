BASE_TIMESTAMP \d{4}-\d{2}-\d{2}\s\d{2}:\d{2}:\d{2}.\d{6}
MSG_ID X'[a-f\d]{48}'
BODY .+
BIN \dx[\D\d]{9}
VALUE \'%{WORD}\'
GROUP_NAME = \[\'[\D\d]*\'\s:\s%{BIN}\]
END_TYPE \([\D\d]*\)
START_TYPE \(BIN:%{WORD}\)
KEY_VAL %{START_TYPE}:%{WORD}\s+=\s%{VALUE}\s%{END_TYPE}
GROUP (%{GROUP_NAME}\n%{KEY_VAL}*\n)
ARG %{WORD}:\sX%{VALUE}
ARGS (%{ARG:args}\s)*%{ARG:arg}?
#LOG %{BASE_TIMESTAMP}\s(%{ARG}\s)*%{ARG}?\s%{GROUP}
